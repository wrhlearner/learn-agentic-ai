    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debugger 3.1: Ollama multi-modality image to text",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/3_1_multimodality_image2text.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Debugger 2.4: SQL Agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/2_4_sql_agent.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Debugger 2.3: agent supervisor",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/2_3_agent_supervisor.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Debugger 2.2: agentic RAG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/2_2_agentic_rag.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Debugger 0: quick start",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/0_quick_start.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        }
    ]
}